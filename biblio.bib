	Journal = {Journal of Archaeological Method and Theory},
	Number = {4},
	Pages = {601--611},
	Title = {Landscape Phenomenology, GIS and the Role of Affordance},
	Volume = {19},
	Year = {2012},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s10816-012-9137-4}}

